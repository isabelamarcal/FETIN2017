<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/menu.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>medicoDashboard</from-outcome>
            <to-view-id>/medico/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pacienteDashboard</from-outcome>
            <to-view-id>/paciente/dashboard.xhtml</to-view-id>
        </navigation-case>
        
        
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/medico/dashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>medicoView</from-outcome>
            <to-view-id>/medico/read.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/medico/read.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>medicoUpdate</from-outcome>
            <to-view-id>/medico/update.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
 
    <navigation-rule>
        <from-view-id>/medico/update.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>UpdateToRead</from-outcome>
            <to-view-id>/medico/read.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/medico/read.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>UpdateToIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paciente/dashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pacienteView</from-outcome>
            <to-view-id>/paciente/read.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/paciente/read.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pacienteUpdate</from-outcome>
            <to-view-id>/paciente/update.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/paciente/read.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>UpdateToIndexP</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>UpdateToReadP</from-outcome>
            <to-view-id>/paciente/read.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paciente/create.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>PacienteLogin</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/medico/create.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>MedicoLogin</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
